			<data source="060R.c" name="c" />
		</combine>
		<combine name="${name}" value="${d}" reset="true">
			<data source="060R.4" name="name">
				<lookup>
					<entry name="datl" value="gnd:birthAndDeath" />
					<!-- <entry name="datx" value="gnd:dateOfBirthAndDeath" /> -->
				</lookup>
			</data>
			<data source="060R.d" name="d">
				<compose prefix="ca. " />
			</data>
		</combine>

