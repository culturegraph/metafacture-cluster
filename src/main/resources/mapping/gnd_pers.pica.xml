		<data source="_id" name="cg:type">
			<constant value="person" />
		</data>

		<!-- 043 -->
		<data source="042B.a" name="cg:geo">
			<case to="upper" />
			<lookup in="countries" />
		</data>


		<!-- 260 -->
		<!-- <data source="041O.0" name="gnd:complexSeeReferenceSubject"/> -->
